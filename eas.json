{
  "cli": {
    "version": ">= 16.7.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": false,
      "distribution": "internal",
      "env": {
        "EXPO_ANDROID_BUILD_JAVA_VERSION": "17"
      }
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_USE_LEGACY_PEER_DEPS": "1",
        "EXPO_ANDROID_BUILD_JAVA_VERSION": "17"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "EXPO_USE_LEGACY_PEER_DEPS": "1",
        "EXPO_ANDROID_BUILD_JAVA_VERSION": "17"
      }
    }
  },
  "submit": {
    "production": {}
  }
}